@model IEnumerable<object>
@{   
   // List<FitFriend.Models.Food> Food = Model.ToList()[0] as List<FitFriend.Models.Food>;
 //   List<FitFriend.Models.Exercises> Exercises = Model.ToList()[1] as List<FitFriend.Models.Exercises>;
}
<h2>Track Calories and Exercises</h2>
<div class="row">
<div class="col-lg-5">
    <a href="../Foods/Create" class="btn btn-danger btn-sm">
        <span class="glyphicon glyphicon-plus"></span><strong> Add a Food</strong>
    </a><br /><br />
    <a href="../Foods/" class="btn btn-success btn-sm">
        <span class="glyphicon glyphicon-edit"></span><strong> Edit All Foods</strong>
    </a><br /><br />
    <table class="table">
        <tr>
            <th>Food Name</th>
            <th>Description</th>
            <th>Calories</th>
        </tr>
       
       @foreach (var item in Food)
          {
            <tr>
                <td>@item.Name</td>
                <td>@item.Description</td>
                <td>@item.Calories</td>
            </tr>
                    }
        
                
    </table>
</div>
<div class="col-lg-2"></div>
<div class="col-lg-5">
    <a href="../Exercises/Create" class="btn btn-warning btn-sm">
        <span class="glyphicon glyphicon-plus"></span><strong> Add an Exercise</strong>
    </a><br /><br />
    <a href="../Exercises/" class="btn btn-primary btn-sm">
        <span class="glyphicon glyphicon-edit"></span><strong> Edit All Exercises</strong>
    </a><br /><br />
<table class="table">
    <tr>
        <th>Exercise Name</th>
        <th>Description</th>
        <th>Calories Burned</th>
    </tr>
    @foreach (var item in Exercises)
    {
        <tr>
            <td>@item.Name</td>
            <td>@item.Description</td>
            <td>@item.CaloriesBurned</td>
        </tr>
    }
</table>
</div>
</div>
